{
  "name": "reactjs-seed",
  "version": "0.0.0",
  "dependencies": {
    "bootstrap-sass": "3.3.3",
    "es6-promise": "~2.0.1",
    "font-awesome": "~4.3.0",
    "immutable": "^3.6.2",
    "jquery": "2.1.2",
    "lodash": "3.4.0",
    "moment": "2.9.0",
    "react": "0.12.2",
    "react-router": "~0.12.4",
    "reflux": "0.2.3",
    "validator": "3.23.0"
  },
  "devDependencies": {
    "babelify": "5.0.4",
    "babel-eslint": "1.0.12",
    "browserify": "~7.1.0",
    "coffeeify": "~1.0.0",
    "del": "~0.1.3",
    "event-stream": "~3.0.5",
    "gulp": "~3.8.9",
    "gulp-connect": "2.2.0",
    "gulp-foreach": "*",
    "gulp-if": "1.2.5",
    "gulp-eslint": "0.6.0",
    "gulp-karma": "0.0.1",
    "gulp-notify": "~1.5.0",
    "gulp-rename": "1.2.0",
    "gulp-ruby-sass": "~0.7.1",
    "gulp-streamify": "0.0.4",
    "gulp-uglify": "~1.1.0",
    "gulp-util": "~3.0.0",
    "karma": "~0.12.28",
    "karma-browserify": "~3.0.1",
    "karma-chrome-launcher": "~0.1.5",
    "karma-firefox-launcher": "~0.1.3",
    "karma-mocha": "0.1.9",
    "karma-phantomjs-launcher": "0.1.4",
    "karma-sinon": "1.0.4",
    "phantomjs": "1.9.12",
    "pretty-hrtime": "*",
    "reactify": "~1.0.0",
    "run-sequence": "~0.3.5",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~2.3.0"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      "reactify",
      "babelify"
    ]
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "gulp test"
  },
  "eslintConfig": {
    "env": {
      "es6":true,
      "browser":true,
      "node":true
    },
    "ecmaFeatures": {
      "modules":true,
      "jsx":true
    },
    "rules": {
      "new-cap" : [2, {"newIsCap": true, "capIsNew": false}],
      "no-unused-vars" : [1, { "vars": "all" }],
      "no-underscore-dangle":false,
      "key-spacing": [1, { "align":"colon" }],
      "quotes": [1, "double", "avoid-escape"]
    }
  }
}
